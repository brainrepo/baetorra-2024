{
  "name": "directus-extension-baetorra-booking",
  "version": "1.0.0",
  "dependencies": {
    "@directus/api": "^11.0.1",
    "date-fns": "^2.29.3",
    "directus": "^10.2.1",
    "express": "^4.18.2",
    "frappe-gantt": "^0.6.1",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "marked": "^4.3.0",
    "qrcode.vue": "^3.4.0",
    "sass": "^1.62.0"
  },
  "directus:extension": {
    "host": "^9.22.4",
    "type": "bundle",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "entries": [
      {
        "type": "layout",
        "name": "supplier-prices",
        "source": "src/supplier-prices/index.ts"
      },
      {
        "type": "layout",
        "name": "supplier-availability",
        "source": "src/supplier-availability/index.ts"
      },
      {
        "type": "endpoint",
        "name": "booking-endpoints",
        "source": "src/booking-endpoints/index.ts"
      },
      {
        "type": "module",
        "name": "booking-module",
        "source": "src/booking-module/index.ts"
      }
    ]
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "add": "directus-extension add",
    "test": "jest"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "9.23.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.15.11",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "vue": "^3.2.47"
  }
}
